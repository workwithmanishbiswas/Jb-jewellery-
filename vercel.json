{
  "version": 2,
  "public": true,
  "buildCommand": "true",
  "env": {},
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    },
    {
      "src": "/css/(.+\\.css)$",
      "dest": "public/css/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/js/(.+\\.js)$",
      "dest": "public/js/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/images/(.+)$",
      "dest": "public/images/$1",
      "headers": {
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.+\\.html)$",
      "dest": "public/$1"
    },
    {
      "src": "/$",
      "dest": "public/index.html"
    },
    {
      "src": "/([^/]+)$",
      "dest": "public/$1.html"
    },
    {
      "src": "/.*",
      "dest": "public/index.html"
    }
  ]
}
