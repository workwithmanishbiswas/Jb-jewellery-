{
  "version": 2,
  "public": true,
  "buildCommand": "true",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/css/(.*\\.css)$",
      "dest": "frontend/css/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/js/(.*\\.js)$",
      "dest": "frontend/js/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/images/(.*)",
      "dest": "frontend/images/$1"
    },
    {
      "src": "/(.*\\.html)$",
      "dest": "frontend/$1"
    },
    {
      "src": "/$",
      "dest": "frontend/index.html"
    },
    {
      "src": "/(.*)$",
      "dest": "frontend/index.html"
    }
  ]
}
